            &lt;/a&gt;
          &lt;/b:if&gt;
        &lt;/div&gt;
      &lt;/b:if&gt;

      &lt;div class='footer'&gt;
        &lt;b:if cond='data:post.embedCommentForm'&gt;
          &lt;b:if cond='data:post.allowNewComments'&gt;
            &lt;b:include data='post' name='commentForm'/&gt;
