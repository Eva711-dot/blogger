        &lt;b:widget id='ReportAbuse1' locked='true' title='' type='ReportAbuse' visible='true'&gt;
          &lt;b:includable id='main'&gt;
  &lt;b:include name='reportAbuse'/&gt;
&lt;/b:includable&gt;
        &lt;/b:widget&gt;
      &lt;/b:section&gt;
    &lt;/aside&gt;

